{
  "manifest_version": 2,
  "name": "GitHub Pull Request Enhancer",
  "description": "Adds functionality to GitHub's pull request pages that increase",
  "version": "0.0.0.2",
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "tabs",
    "activeTab",
    "https://*/*/*/pull/*"
  ]
}